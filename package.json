{
	"name": "interpolatr",
	"version": "0.0.1",
	"description": "Parse any value in search of placeholders within strings and replace them accordingly.",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"scripts": {
		"clean": "rimraf ./dist && rimraf ./coverage",
		"lint": "eslint 'src/**/*.ts'",
		"lint:fix": "eslint --fix 'src/**/*.ts'",
		"compile:watch": "tsc --watch",
		"compile": "tsc",
		"prebuild": "npm run clean && npm run lint && npm run test",
		"prepare": "npm run build",
		"build": "npm run compile",
		"test": "jest --verbose --coverage"
	},
	"devDependencies": {
		"@types/lodash.isarray": "^4.0.6",
		"@types/lodash.clonedeep": "^4.5.6",
		"@types/lodash.isboolean": "^3.0.6",
		"@types/lodash.isnull": "^3.0.6",
		"@types/lodash.isnumber": "^3.0.6",
		"@types/lodash.isplainobject": "^4.0.6",
		"@types/lodash.isstring": "^4.0.6",
		"@types/lodash.isundefined": "^3.0.6",
		"@types/faker": "^4.1.12",
		"@types/jest": "^26.0.9",
		"@types/picomatch": "^2.2.1",
		"@types/rfdc": "^1.1.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-prettier": "^3.1.4",
		"faker": "^4.1.0",
		"jest": "^26.2.2",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.1.4",
		"ts-node": "^8.10.2",
		"typescript": "^4.1.0-dev.20200808"
	},
	"dependencies": {
		"lodash.isarray": "^4.0.0",
		"lodash.clonedeep": "^4.5.0",
		"lodash.isboolean": "^3.0.3",
		"lodash.isnull": "^3.0.0",
		"lodash.isnumber": "^3.0.3",
		"lodash.isplainobject": "^4.0.6",
		"lodash.isstring": "^4.0.1",
		"lodash.isundefined": "~3.0.1",
		"picomatch": "^2.2.2",
		"rfdc": "^1.1.4"
	},
	"author": "Lazhar Ichir",
	"license": "GNU General Public License v3.0",
	"homepage": "https://github.com/lazharichir/interpolatr#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lazharichir/interpolatr.git"
	},
	"bugs": {
		"url": "https://github.com/lazharichir/interpolatr/issues"
	},
	"keywords": [
		"map",
		"string interpolation",
		"interpolation",
		"deep",
		"nested",
		"pattern matching",
		"deep map"
	]
}